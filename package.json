{
  "name": "isp-site",
  "version": "5.0.1+1725031812",
  "homepage": "https://inst.bid/#/",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "biome:check": "pnpm lint && pnpm check && pnpm format",
    "biome:apply": "pnpm lint:apply && pnpm check:apply && pnpm format:apply",
    "build": "pnpm build:preview && pnpm build:git",
    "build:git": "git add . && git commit -m \"Build v$npm_package_version\" && git push",
    "build:js": "vite build",
    "build:preview": "pnpm biome:apply && pnpm sec && pnpm build:version && pnpm build:ts && pnpm build:js",
    "build:ts": "tsc",
    "build:version": "node-jq -r '.version=$ARGS.positional[0]+\"+\"+$ARGS.positional[1]' package.json --args \"$(pnpm -s env:semver)\" \"$(date +%s)\" | sponge package.json",
    "check": "biome check",
    "check:apply": "biome check --write",
    "env:package": "node-jq '.' package.json",
    "env:biome": "biome rage",
    "env:buildtime": "date -r $(node-jq -r '(.version | split(\"+\"))[1]' package.json)",
    "env:semver": "node-jq -r '(.version | split(\"+\"))[0]' package.json",
    "env:version": "echo \"$npm_package_version\"",
    "format": "biome format",
    "format:apply": "biome format --write",
    "lint": "biome lint",
    "lint:apply": "biome lint --write",
    "preview": "pnpm build:preview && vite preview",
    "push": "pnpm build && {pnpm tag || printf \"git tag already exists.\r\n\";} && gh-pages -t -m \"Deploy v$(pnpm -s env:version)\" -d dist",
    "sec": "pnpm sec:audit && pnpm sec:scan",
    "sec:audit": "snyk test && pnpm audit",
    "sec:scan": "snyk code test",
    "tag": "git tag isp-site-v$(pnpm -s env:semver) && git push origin isp-site-v$(pnpm -s env:semver)"
  },
  "dependencies": {
    "@instructure/ui": "10.2.1",
    "react": "19.0.0-rc-3dfd5d9e-20240910",
    "react-dom": "19.0.0-rc-3dfd5d9e-20240910",
    "react-markdown": "^9.0.1",
    "react-router-dom": "^6.26.2",
    "rehype-raw": "^7.0.0",
    "remark-gemoji": "^8.0.0",
    "remark-gfm": "^4.0.0"
  },
  "devDependencies": {
    "@babel/plugin-syntax-import-attributes": "^7.25.6",
    "@biomejs/biome": "1.8.3",
    "@instructure/browserslist-config-canvas-lms": "^2.0.0",
    "@instructure/shared-types": "^9.6.0",
    "@octokit/types": "^13.5.0",
    "@types/react": "npm:types-react@19.0.0-rc.1",
    "@types/react-dom": "npm:types-react-dom@19.0.0-rc.1",
    "@vitejs/plugin-react": "^4.3.1",
    "babel-plugin-react-compiler": "0.0.0-experimental-e68eda9-20240829",
    "gh-pages": "^6.1.1",
    "lightningcss": "^1.26.0",
    "node-jq": "^4.4.0",
    "snyk": "^1.1293.0",
    "snyk-policy": "^3.0.0",
    "terser": "^5.32.0",
    "typescript": "^5.6.2",
    "vite": "^5.4.3"
  },
  "engines": {
    "node": ">=22.7.0",
    "pnpm": ">=9.9.0"
  },
  "packageManager": "pnpm@9.10.0+sha512.73a29afa36a0d092ece5271de5177ecbf8318d454ecd701343131b8ebc0c1a91c487da46ab77c8e596d6acf1461e3594ced4becedf8921b074fbd8653ed7051c",
  "pnpm": {
    "overrides": {
      "react": "^19.0.0-rc-d48603a5-20240813",
      "react-dom": "^19.0.0-rc-d48603a5-20240813",
      "@instructure/ui-test-sandbox>sinon": "^17.0.1",
      "gh-pages>globby": "11.1.0",
      "node-jq>tempfile": "^5.0.0",
      "bin-build>tempfile": "^5.0.0",
      "bin-build>download": "^8.0.0",
      "download>got": "^14.4.1"
    }
  },
  "browserslist": [
    "extends @instructure/browserslist-config-canvas-lms"
  ]
}
